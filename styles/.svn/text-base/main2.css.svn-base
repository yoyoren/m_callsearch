body{font-size:12px;color:#491903; background:#fffdde;margin:5px;}
td,form{font-size:12px;}
a{line-height:15px;height:15px;text-decoration:none;}
a:hover{color:#CC0000;line-height:15px;height:15px;text-decoration:none;}
input {border:#660000 1px solid;color:#491903;font-size:18px;height:30px;line-height:30px;}
	input:hover{background: #336699;color: #fff;cursor:hand;}
.main{font-size:18px;height:30px;line-height:30px;text-align:center; float:left;}
.list_url{font-size:18px;height:30px;line-height:30px;text-align:center;text-decoration:none;}
	.list_url a{background-color:#CCC;color:#491903;padding:5px;font-size:18px;height:30px;line-height:30px;}
	.list_url a:hover{background-color:#660000;color:#fff;padding:5px;font-size:18px;height:30px;line-height:30px;}
input {border:#660000 0px solid;color:#491903;font-size:12px;height:25px;line-height:25px;}
input:hover{background: #336699;color:#fff;cursor:hand;}
.resizeDivClass{position:relative;background-color:red;width:2px;z-index:1;left:expression(this.parentElement.offsetWidth-1);cursor:e-resize;}
.pagesize{font-size:16px; height:50px;line-height:50px;cursor:hand;}
.search-div {line-height:25px;font-size:16px;}
.inpf {height:18px; line-height:18px; font-size:12px; border:#491903 1px solid;}	

h1 {
  background: #b28146;
  border: 1px solid #491903;
  color: #fff;
  font-size: 12px;
  padding: 1px 10px;
  margin:0;
  overflow:hidden;
  height:25px;
  line-height:16px;
}

h1 span{zoom:1;}
h1 .action-span {float:right; padding-left:10px;}
h1 .action-span a {color: #fff;font-size: 12px;
 font-weight: 400;
  text-decoration: none;
  display:block;
  padding:2px 5px 2px 23px;
  *padding:4px 5px 2px 23px;
  border:1px solid #fff;
  border-right:2px solid #fff;
  border-bottom:2px solid #fff;
  background:#693012  no-repeat 3px center;
  *background:#693012  no-repeat 3px 3px;
}

h1 .action-span a:hover {
  border:1px solid #693012;
  border-right:2px solid #693012;
  border-bottom:2px solid #693012;
  background:#FFF  no-repeat 3px center;
  *background:#FFF no-repeat 3px 3px;
  color:#666;
  text-decoration:none;
}

h1 .action-span1 {
  float:left;
}
