}
/* 链接
----------------------------------------------------------*/
a:link {color: #a76d1a; text-decoration: underline;}

a:visited {color: #6164bc; text-decoration: underline;}

a:hover {color: #999999; text-decoration: none;}

a:active {color: #a76d1a; text-decoration: underline;}



/* 表单元素 
----------------------------------------------------------*/
fieldset 
{
    margin: 6px 0;
    padding: 6px;
    border: 1px solid #CCC;
}

fieldset p 
{
    margin: 2px 12px 10px 10px;
}

fieldset label 
{
    display: block;
}

fieldset label.inline 
{
    display: inline;
}

legend 
{
    font-size: 1.1em;
    font-weight: 600;
    padding: 2px 4px 8px 4px;
}
input { padding:1px 2px;
}
input[type="text"] 
{
    width:120px;
    border: 1px solid #CCC;
}

input[type="password"] 
{
    width: 150px;
    border: 1px solid #CCC;
}

